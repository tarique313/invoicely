<div class="container">
<div class="row">
<p id="notice"><%= notice %></p>       
<center>  <h4>Listing Dispatches</h4></center>
<hr>
<table class="striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Company</th>
      <th>Tax</th>
      <th>Salesperson</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dispatches.each do |dispatch| %>
      <tr>
        <td><%= dispatch.date %></td>
        <td><%= dispatch.company %></td>
        <td><%= dispatch.tax %></td>
        <td><%= dispatch.salesperson %></td>
        <td><%= link_to 'Show', dispatch , class: "waves-effect waves-light btn"%></td>
        <td><%= link_to 'Edit', edit_dispatch_path(dispatch) , class: "waves-effect waves-light btn"%></td>
        <td><%= link_to 'Destroy', dispatch, method: :delete, data: { confirm: 'Are you sure?' } , class: " red lighten-2 waves-effect waves-light btn"%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dispatch', new_dispatch_path, class: " blue darken-3 waves-effect waves-light btn" %>
</div>
</div>

